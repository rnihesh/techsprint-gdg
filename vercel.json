{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd apps/web && pnpm build",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": null,
  "outputDirectory": "apps/web/.next",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/node/:path*"
    }
  ],
  "functions": {
    "api/node/[...path].ts": {
      "maxDuration": 30
    }
  }
}
